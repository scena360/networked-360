{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "modules": true
      }
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-unused-vars": ["error", { "args": "none" }]
  }
}
